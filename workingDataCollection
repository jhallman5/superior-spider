function user(username, email, password) {
  this.username = username;
  this.email = email;
  this.password = password;
}
var users =[];

function createUser(newUser) {
  newUser = new user("U4","U5", "U6")  ;
  users.push(newUser);
  return newUser;//Not sure if we need this
}



